
@prefix modgui: <http://moddevices.com/ns/modgui#> .
@prefix lv2:    <http://lv2plug.in/ns/lv2core#> .

<https://github.com/steveb/firce/vintage_mics>
    modgui:gui [
        modgui:resourcesDirectory <modgui> ;
        modgui:iconTemplate <modgui/icon-vintage-mics.html> ;
        modgui:stylesheet <modgui/stylesheet-vintage-mics.css> ;
        modgui:screenshot <modgui/screenshot-vintage-mics.png> ;
        modgui:thumbnail <modgui/thumbnail-vintage-mics.png> ;
        modgui:brand "Gula" ;
        modgui:label "Vintage Mics" ;
        modgui:model "boxy" ;
        modgui:panel "1-select-1-knob" ;
        modgui:color "zinc" ;
        modgui:knob "black" ;
        modgui:port [
            lv2:index 0 ;
            lv2:symbol "ir_1" ;
            lv2:name "Impulse Response" ;
        ] , [
            lv2:index 1 ;
            lv2:symbol "gain_0" ;
            lv2:name "Gain" ;
        ] ;
    ] .
