@prefix modgui: <http://moddevices.com/ns/modgui#> .
@prefix lv2:    <http://lv2plug.in/ns/lv2core#> .

<https://github.com/steveb/gula-plugins/splits>
    modgui:gui [
        modgui:resourcesDirectory <modgui> ;
        modgui:iconTemplate <modgui/icon-the-splits.html> ;
        modgui:stylesheet <modgui/stylesheet-the-splits.css> ;
        modgui:screenshot <modgui/screenshot-the-splits.png> ;
        modgui:thumbnail <modgui/thumbnail-the-splits.png> ;
        modgui:brand "GULA" ;
        modgui:label "The Splits" ;
        modgui:port [
            lv2:index 0 ;
            lv2:symbol "split_0" ;
            lv2:name "Split" ;
        ] , [
            lv2:index 1 ;
            lv2:symbol "spread_1" ;
            lv2:name "Spread" ;
        ] ;
    ] .