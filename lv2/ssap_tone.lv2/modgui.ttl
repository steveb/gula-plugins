@prefix modgui: <http://moddevices.com/ns/modgui#> .
@prefix lv2:    <http://lv2plug.in/ns/lv2core#> .

<https://github.com/steveb/gula-plugins/ssap_tone>
    modgui:gui [
        modgui:resourcesDirectory <modgui> ;
        modgui:iconTemplate <modgui/icon-ssap-tone.html> ;
        modgui:stylesheet <modgui/stylesheet-ssap-tone.css> ;
        modgui:screenshot <modgui/screenshot-ssap-tone.png> ;
        modgui:thumbnail <modgui/thumbnail-ssap-tone.png> ;
        modgui:brand "GULA" ;
        modgui:label "SSaP Tone" ;
        modgui:model "boxy" ;
        modgui:panel "7-knobs" ;
        modgui:color "black" ;
        modgui:knob "black" ;
        modgui:port [
            lv2:index 0 ;
            lv2:symbol "bass_1" ;
            lv2:name "Bass" ;
        ] , [
            lv2:index 1 ;
            lv2:symbol "mid_2" ;
            lv2:name "Mid" ;
        ] , [
            lv2:index 2 ;
            lv2:symbol "mid_q_3" ;
            lv2:name "Mid Q" ;
        ] , [
            lv2:index 3 ;
            lv2:symbol "treble_4" ;
            lv2:name "Treble" ;
        ] , [
            lv2:index 4 ;
            lv2:symbol "freq_0" ;
            lv2:name "Freq" ;
        ] ;
    ] .