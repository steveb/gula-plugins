@prefix modgui: <http://moddevices.com/ns/modgui#> .
@prefix lv2:    <http://lv2plug.in/ns/lv2core#> .

<https://github.com/steveb/gula-plugins/gods_cab>
    modgui:gui [
        modgui:resourcesDirectory <modgui> ;
        modgui:iconTemplate <modgui/icon-gods-cab.html> ;
        modgui:stylesheet <modgui/stylesheet-gods-cab.css> ;
        modgui:screenshot <modgui/screenshot-gods-cab.png> ;
        modgui:thumbnail <modgui/thumbnail-gods-cab.png> ;
        modgui:brand "Gula" ;
        modgui:label "God's Cab" ;
        modgui:model "boxy" ;
        modgui:panel "1-select-1-knob" ;
        modgui:color "zinc" ;
        modgui:knob "aluminium" ;
        modgui:port [
            lv2:index 0 ;
            lv2:symbol "ir_1" ;
            lv2:name "Impulse Response" ;
        ] ;
    ] .