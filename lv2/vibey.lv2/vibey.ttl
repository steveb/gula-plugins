@prefix doap:  <http://usefulinc.com/ns/doap#> .
@prefix foaf:  <http://xmlns.com/foaf/0.1/> .
@prefix lv2:   <http://lv2plug.in/ns/lv2core#> .
@prefix ui:    <http://lv2plug.in/ns/extensions/ui#> .
@prefix epp:   <http://lv2plug.in/ns/ext/port-props#> .
@prefix atom:  <http://lv2plug.in/ns/ext/atom#> .
@prefix rdf:   <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs:  <http://www.w3.org/2000/01/rdf-schema#> .
@prefix units: <http://lv2plug.in/ns/extensions/units#> .
<https://github.com/steveb/gula-plugins/vibey>
       a lv2:Plugin ;
       doap:name "The Vibey" ;
       lv2:binary <vibey.so> ;
       lv2:optionalFeature epp:supportsStrictBounds ;
       lv2:optionalFeature lv2:hardRtCapable ;
       doap:maintainer [ foaf:name "Steve Baker" ] ;
       doap:revision "1.0.0" ;
       doap:license "GPLv3" ;
rdfs:comment """
A combination of vibrato and tremolo. The depth of each can be controlled and
the phase offset of the vibrato can be set relative to the tremolo.
""";
    lv2:port [
	a lv2:InputPort ;
	a lv2:ControlPort ;
	lv2:index 0 ;
	lv2:symbol "rate_0" ;
	lv2:name "Rate" ;
        lv2:portProperty epp:hasStrictBounds ;
	lv2:default 4 ;
	lv2:minimum 0.4 ;
	lv2:maximum 10 ;
	units:unit units:hz ;
    ] , [
	a lv2:InputPort ;
	a lv2:ControlPort ;
	lv2:index 1 ;
	lv2:symbol "trem_depth_1" ;
	lv2:name "Tremelo Depth" ;
        lv2:portProperty epp:hasStrictBounds ;
	lv2:default 0.24 ;
	lv2:minimum 0 ;
	lv2:maximum 1 ;
    ] , [
	a lv2:InputPort ;
	a lv2:ControlPort ;
	lv2:index 2 ;
	lv2:symbol "trem_shape_2" ;
	lv2:name "Tremelo Shape" ;
        lv2:portProperty epp:hasStrictBounds ;
	lv2:default 0.0 ;
	lv2:minimum 0 ;
	lv2:maximum 4.99 ;
    ] , [
	a lv2:InputPort ;
	a lv2:ControlPort ;
	lv2:index 3 ;
	lv2:symbol "vib_depth_3" ;
	lv2:name "Vibrato Depth" ;
        lv2:portProperty epp:hasStrictBounds ;
	lv2:default 0.12 ;
	lv2:minimum 0 ;
	lv2:maximum 1 ;
    ] , [
	a lv2:InputPort ;
	a lv2:ControlPort ;
	lv2:index 4 ;
	lv2:symbol "vib_offset_4" ;
	lv2:name "Vibrato Offset" ;
        lv2:portProperty epp:hasStrictBounds ;
	lv2:default 0 ;
	lv2:minimum 0 ;
	lv2:maximum 1 ;
    ], [
	a lv2:InputPort ;
	a lv2:AudioPort ;
	lv2:index 5 ;
	lv2:symbol "in0" ;
	lv2:name "in0" ;
    ] , [
	a lv2:OutputPort ;
	a lv2:AudioPort ;
	lv2:index 6 ;
	lv2:symbol "out0" ;
	lv2:name "out0" ;
    ] , [
	a lv2:OutputPort ;
	a lv2:AudioPort ;
	lv2:index 7 ;
	lv2:symbol "out0" ;
	lv2:name "out0" ;
    ]
.
