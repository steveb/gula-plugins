@prefix modgui: <http://moddevices.com/ns/modgui#> .
@prefix lv2:    <http://lv2plug.in/ns/lv2core#> .

<https://github.com/steveb/gula-plugins/vibey>
    modgui:gui [
        modgui:resourcesDirectory <modgui> ;
        modgui:iconTemplate <modgui/icon-the-vibey.html> ;
        modgui:stylesheet <modgui/stylesheet-the-vibey.css> ;
        modgui:screenshot <modgui/screenshot-the-vibey.png> ;
        modgui:thumbnail <modgui/thumbnail-the-vibey.png> ;
        modgui:brand "GULA" ;
        modgui:label "The Vibey" ;
        modgui:model "boxy" ;
        modgui:panel "6-knobs" ;
        modgui:color "dots" ;
        modgui:knob "gold" ;
        modgui:port [
            lv2:index 0 ;
            lv2:symbol "trem_depth_0" ;
            lv2:name "T Depth" ;
        ] , [
            lv2:index 1 ;
            lv2:symbol "trem_shape_1" ;
            lv2:name "T Shape" ;
        ] , [
            lv2:index 2 ;
            lv2:symbol "trem_slant_2" ;
            lv2:name "T Slant" ;
        ] , [
            lv2:index 3 ;
            lv2:symbol "vib_depth_3" ;
            lv2:name "V Depth" ;
        ] , [
            lv2:index 4 ;
            lv2:symbol "vib_offset_4" ;
            lv2:name "V Offset" ;
        ] , [
            lv2:index 5 ;
            lv2:symbol "rate_5" ;
            lv2:name "Rate" ;
        ] ;
    ] .